{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "lib": [
            "ES2022"
        ],
        "types": [
            "@cloudflare/workers-types"
        ],
        "strict": true,
        "noEmit": true,
        "allowImportingTsExtensions": true,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "src",
        "**/*.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}