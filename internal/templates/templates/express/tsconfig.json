{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "noEmit": true,
        "lib": [
            "ESNext",
            "WebWorker"
        ],
        "types": [
            "@cloudflare/workers-types",
            "@types/express",
            "@cloudflare/vitest-pool-workers"
        ]
    },
    "include": [
        "src/**/*"
    ]
}